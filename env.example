# Aiven MySQL Database Configuration
# Copy this file to .env.local and fill in your Aiven MySQL credentials

# Aiven MySQL Connection Details
AIVEN_MYSQL_HOST=your-mysql-host.aivencloud.com
AIVEN_MYSQL_PORT=3306
AIVEN_MYSQL_USER=your-username
AIVEN_MYSQL_PASSWORD=your-password
AIVEN_MYSQL_DATABASE=your-database-name

# Next.js Configuration
NEXTAUTH_SECRET=your-nextauth-secret-key-here
NEXTAUTH_URL=http://localhost:3000

# Optional: WebSocket URL for real-time features
NEXT_PUBLIC_WS_URL=wss://your-websocket-server.com
